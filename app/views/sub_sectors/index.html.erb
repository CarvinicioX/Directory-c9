<p id="notice"><%= notice %></p>

<h1>Sub Sectors</h1>

<table>
  <thead>
    <tr>
      <th>Sector</th>
      <th>Num sub sector</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sub_sectors.each do |sub_sector| %>
      <tr>
        <td><%= sub_sector.sector %></td>
        <td><%= sub_sector.num_sub_sector %></td>
        <td><%= sub_sector.name %></td>
        <td><%= link_to 'Show', sub_sector %></td>
        <td><%= link_to 'Edit', edit_sub_sector_path(sub_sector) %></td>
        <td><%= link_to 'Destroy', sub_sector, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sub Sector', new_sub_sector_path %>
