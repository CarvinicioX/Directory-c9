<p id="notice"><%= notice %></p>

<h1>Polling Stations</h1>

<table>
  <thead>
    <tr>
      <th>Sub sector</th>
      <th>Num pollstat</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @polling_stations.each do |polling_station| %>
      <tr>
        <td><%= polling_station.sub_sector %></td>
        <td><%= polling_station.num_pollstat %></td>
        <td><%= polling_station.name %></td>
        <td><%= link_to 'Show', polling_station %></td>
        <td><%= link_to 'Edit', edit_polling_station_path(polling_station) %></td>
        <td><%= link_to 'Destroy', polling_station, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Polling Station', new_polling_station_path %>
